{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "npm install --registry https://registry.npmjs.org",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "20.18.0",
      "NPM_CONFIG_REGISTRY": "https://registry.npmjs.org"
    }
  }
}
